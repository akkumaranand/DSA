#include <bits/stdc++.h>
using namespace std; 

// hamesa pass by reference hi lenaaa 
void printvector( vector<int > &s1){
     cout<<"size"<<s1.size()<<endl;
     for(int i = 0; i<s1.size() ; i++){
        cout<<s1[i]<<" ";
     }
}

int main(){
    // Array of vector 
    // it is 2D vector where no of rows is constant but length of column is  varrying ...
    
    vector<int > s[3];
    for(int i = 0; i< 3 ; ++i){
        int n;
        cout<<"Enter the size of "<<i+1<<"vector"<<endl;
        cin>>n;
        for(int j = 0 ; j<n ; j++){
            int x ;
            cout<<"Enter the value "<<endl;
            cin>>x;
            s[i].push_back(x);
        }

    } 
    for(int i =0 ; i<3; i++){
        printvector(s[i]);
        cout<<endl;
    }



 

  return 0;
}
